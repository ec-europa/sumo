  </xs:element>
  <xs:element name="replicaList">
    <xs:complexType>
      <xs:attribute name="count" type="xs:integer" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="noiseList">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="noise"/>
      </xs:sequence>
      <xs:attribute name="count" type="xs:integer" use="required"/>
    </xs:complexType>
  </xs:element>
